pipeline {
    
    agent any
    
    stages {
        stage('Build') {
            steps {
               echo "this is the build stage"
            }
        }
        stage('Test') {
            steps {
                echo "this is the test stage"
            }
        }
        stage('Deliver') {
            steps {
               echo "this is the deliver stage"
            }
        }
    }
}
